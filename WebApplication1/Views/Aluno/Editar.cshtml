@model Aluno

<form asp-controller="Aluno" asp-action="InserirAluno" method="post">

    <div class="mb-3 col-md-6">
        <label class="form-label"> Nome</label>
        <input type="text" asp-for="Nome" class="form-control col-md-2">
    </div>

    <div class="mb-3  col-md-2">
        <label class="form-label"> Matricula</label>
        <input type="text" asp-for="Matricula" class="form-control col-md-2">
    </div>

    <div class="mb-3  col-md-2">
        <label class="form-label"> Cpf</label>
        <input type="text" asp-for="Cpf" class="form-control col-md-2">
    </div>

    <div class="text-center">
        <button type="submit" class="btn btn-primary">Adicionar</button>
    </div>
</form>